            <tbody>
            <security:authorize ifAllGranted="ROLE_ADMINISTRATOR">
                <tr>
                    <td class="update_table_date ${color}">2008-11-22</td>
                    <td class="${color} update_name"><hxcel:truncate text="com.springsource.server.servlet.admin" maxLength="36"/></td>
                </tr>
