<jsp:root xmlns:jsp="http://java.sun.com/JSP/Page"
          xmlns="http://www.w3.org/1999/xhtml"
          xmlns:spring="http://www.springframework.org/tags"
          xmlns:spring-form="http://www.springframework.org/tags/form"
          xmlns:security="http://www.springframework.org/security/tags"
          xmlns:c="http://java.sun.com/jsp/jstl/core"
          xmlns:x="http://java.sun.com/jsp/jstl/xml"
          xmlns:fmt="http://java.sun.com/jsp/jstl/fmt"
          xmlns:tiles="http://tiles.apache.org/tags-tiles"
          xmlns:fn="http://java.sun.com/jsp/jstl/functions" version="2.0">
    <jsp:directive.page pageEncoding="UTF-8" contentType="text/html; charset=UTF-8"/>

    <div id="${organization.id}_${ol.id}_override">
        <div class="page_header configuration_override">
            <div class="titleName"><spring:message code="page.licensed.application.configuration.overrides" text="Configuration overrides"/></div>
            <div class="newEntity">
                <security:authorize ifAllGranted="ROLE_ADMINISTRATOR">
                    <a href="javascript:bindFormLink({url: '/secure/organization/licensed/application/override/edit.admin?applicationId=${ol.license.application.id}&amp;organizationLicenseId=${ol.id}&amp;organizationId=${organization.id}&amp;platform=${platform}', id: '${organization.id}_${ol.id}_overrideTable', cssClass: 'jqmWindow'})" title="${altNew}"><spring:message code="entity.new" text="NEW"/></a>
                </security:authorize>
            </div>
            <div class="searchField">
                <input type="text" name="search" onkeyup="replaceHtmlContent({url:'/secure/organization/licensed/application/overrides/search.admin?id=${ol.id}&amp;organizationId=${organization.id}&amp;view=organization.licensed.application.overrides.data&amp;platform=${platform}&amp;name=' + this.value, id: '${organization.id}_${ol.id}_overrideTable'});" class="textInput"/>
            </div>
        </div>

        <div class="contentSeparator"><!----></div>
        
        <jsp:include page="organization_licensed_application_overrides_data.jspx" flush="true"/>
    </div>
</jsp:root>